WDIsearch('trade')
var = c('Trade.gdp' = 'TG.VAL.TOTL.GD.PP.ZS',
'Trade.volum' = 'BN.GSR.GNFS.CD',
'GDPpc' = 'NY.GDP.PCAP.KD')
wdi <- WDI(indicator=var,
start=1960,
country = 'all',
end=2020,
extra=TRUE)
wdi %>%
mutate(oecd =
case_when(country == "OECD members" ~ "oecd",
country != "OECD members" ~ "non-oecd")) %>%
ggplot(aes(year, Trade.volumn, color=oecd)) +
geom_smooth(alpha=0.25) +
labs(x="Year",
y="Volume of Trade") +
theme(legend.title = element_blank()) +
scale_color_manual(values=c("red", "blue")) +
theme(legend.text = element_text(size=13),
axis.text=element_text(size=15),
axis.title=element_text(size=15))
