@article{Konig2013,
	Annote = {10.1093/pan/mpt003},
	Author = {K{\"o}nig, Thomas and Marbach, Moritz and Osnabr{\"u}gge, Moritz},
	Date-Added = {2015-08-28 19:05:45 +0000},
	Date-Modified = {2015-08-28 19:05:45 +0000},
	Journal = {Political Analysis},
	Journal1 = {Political Analysis},
	Number = {4},
	Pages = {468--491},
	Title = {Estimating Party Positions across Countries and Time---A Dynamic Latent Variable Model for Manifesto Data},
	Ty = {JOUR},
	Url = {http://pan.oxfordjournals.org/content/21/4/468.abstract N2 - This article presents a new method for estimating positions of political parties across country- and time-specific contexts by introducing a latent variable model for manifesto data. We estimate latent positions and exploit bridge observations to make the scales comparable. We also incorporate expert survey data as prior information in the estimation process to avoid ex post facto interpretation of the latent space. To illustrate the empirical contribution of our method, we estimate the left-right positions of 388 parties competing in 238 elections across twenty-five countries and over sixty years. Compared to the puzzling volatility of existing estimates, we find that parties more modestly change their left-right positions over time. We also show that estimates without country- and time-specific bias parameters risk serious, systematic bias in about two-thirds of our data. This suggests that researchers should carefully consider the comparability of party positions across countries and/or time.},
	Volume = {21},
	Year = {Autumn 2013},
	Bdsk-Url-1 = {http://pan.oxfordjournals.org/content/21/4/468.abstract%20N2%20-%20This%20article%20presents%20a%20new%20method%20for%20estimating%20positions%20of%20political%20parties%20across%20country-%20and%20time-specific%20contexts%20by%20introducing%20a%20latent%20variable%20model%20for%20manifesto%20data.%20We%20estimate%20latent%20positions%20and%20exploit%20bridge%20observations%20to%20make%20the%20scales%20comparable.%20We%20also%20incorporate%20expert%20survey%20data%20as%20prior%20information%20in%20the%20estimation%20process%20to%20avoid%20ex%20post%20facto%20interpretation%20of%20the%20latent%20space.%20To%20illustrate%20the%20empirical%20contribution%20of%20our%20method,%20we%20estimate%20the%20left-right%20positions%20of%20388%20parties%20competing%20in%20238%20elections%20across%20twenty-five%20countries%20and%20over%20sixty%20years.%20Compared%20to%20the%20puzzling%20volatility%20of%20existing%20estimates,%20we%20find%20that%20parties%20more%20modestly%20change%20their%20left-right%20positions%20over%20time.%20We%20also%20show%20that%20estimates%20without%20country-%20and%20time-specific%20bias%20parameters%20risk%20serious,%20systematic%20bias%20in%20about%20two-thirds%20of%20our%20data.%20This%20suggests%20that%20researchers%20should%20carefully%20consider%20the%20comparability%20of%20party%20positions%20across%20countries%20and/or%20time.}}

@article{Perez2011,
	Annote = {10.1093/pan/mpr029},
	Author = {P{\'e}rez, Efr{\'e}n O.},
	Date-Added = {2015-08-28 19:05:29 +0000},
	Date-Modified = {2015-08-28 19:05:29 +0000},
	Journal = {Political Analysis},
	Journal1 = {Political Analysis},
	Number = {4},
	Pages = {434--454},
	Title = {The Origins and Implications of Language Effects in Multilingual Surveys: A MIMIC Approach with Application to Latino Political Attitudes},
	Ty = {JOUR},
	Url = {http://pan.oxfordjournals.org/content/19/4/434.abstract N2 - The study of Latino public opinion has renewed interest in the relationship between language and survey response. However, extant research generally relies on statistical methods that cannot distinguish between two related yet distinct types of language effects in Latino surveys: (1) differences in attitude and (2) differences in measures of attitude. The former reflects varied levels of a latent attitude between English and Spanish interviewees. The latter---formally known as Differential Item Functioning (DIF)---refers to linguistic differences in the interpretation of survey items, which lead Latino respondents to misreport their level of attitude. This paper proposes Multiple Indicators Multiple Causes models to decouple these two types of language effects. Using this modeling framework, I examine language differences in measures of subjective and factual political attitudes from the Latino National Survey (2006). I find that the language of interview systematically colors Latinos' interpretation of survey items, even after controlling for measurement error and individual differences in the latent variable being assessed. I then show through an applied analysis how ignoring language DIF can yield misleading inferences about hypothesized relationships between variables. Together, these findings highlight a need for greater theoretical work on the psychological origins of language effects in multilingual political surveys.},
	Volume = {19},
	Year = {Autumn 2011},
	Bdsk-Url-1 = {http://pan.oxfordjournals.org/content/19/4/434.abstract%20N2%20-%20The%20study%20of%20Latino%20public%20opinion%20has%20renewed%20interest%20in%20the%20relationship%20between%20language%20and%20survey%20response.%20However,%20extant%20research%20generally%20relies%20on%20statistical%20methods%20that%20cannot%20distinguish%20between%20two%20related%20yet%20distinct%20types%20of%20language%20effects%20in%20Latino%20surveys:%20(1)%20differences%20in%20attitude%20and%20(2)%20differences%20in%20measures%20of%20attitude.%20The%20former%20reflects%20varied%20levels%20of%20a%20latent%20attitude%20between%20English%20and%20Spanish%20interviewees.%20The%20latter%E2%80%94formally%20known%20as%20Differential%20Item%20Functioning%20(DIF)%E2%80%94refers%20to%20linguistic%20differences%20in%20the%20interpretation%20of%20survey%20items,%20which%20lead%20Latino%20respondents%20to%20misreport%20their%20level%20of%20attitude.%20This%20paper%20proposes%20Multiple%20Indicators%20Multiple%20Causes%20models%20to%20decouple%20these%20two%20types%20of%20language%20effects.%20Using%20this%20modeling%20framework,%20I%20examine%20language%20differences%20in%20measures%20of%20subjective%20and%20factual%20political%20attitudes%20from%20the%20Latino%20National%20Survey%20(2006).%20I%20find%20that%20the%20language%20of%20interview%20systematically%20colors%20Latinos%E2%80%99%20interpretation%20of%20survey%20items,%20even%20after%20controlling%20for%20measurement%20error%20and%20individual%20differences%20in%20the%20latent%20variable%20being%20assessed.%20I%20then%20show%20through%20an%20applied%20analysis%20how%20ignoring%20language%20DIF%20can%20yield%20misleading%20inferences%20about%20hypothesized%20relationships%20between%20variables.%20Together,%20these%20findings%20highlight%20a%20need%20for%20greater%20theoretical%20work%20on%20the%20psychological%20origins%20of%20language%20effects%20in%20multilingual%20political%20surveys.}}

