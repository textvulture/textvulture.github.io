	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {50},
	Pages = {e2102144118},
	Publisher = {National Academy of Sciences},
	Title = {Polarization and tipping points},
	Url = {https://www.pnas.org/content/118/50/e2102144118},
	Volume = {118},
	Year = {2021},
	Bdsk-File-1 = {},
