	Eprint = {http://cps.sagepub.com/cgi/reprint/49/7/811},
	Journal = {Comparative Political Studies},
	Number = {7},
	Pages = {811--840},
	Title = {The Political Economy of the Euro Crisis},
	Url = {http://cps.sagepub.com/cgi/content/abstract/49/7/811},
	Volume = {49},
	Year = {2016},
	Bdsk-Url-1 = {http://cps.sagepub.com/cgi/content/abstract/49/7/811},
