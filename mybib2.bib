	Journal = {{American Political Science Review}},
	Number = {{1}},
	Pages = {144--168},
	Title = {{The Impact of Recentralization on Public Services: A Difference- in- Differences Analysis of the Abolition of Elected Councils in Vietnam}},
	Volume = {{108}},
