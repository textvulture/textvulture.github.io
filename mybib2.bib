@article{Brancati2014,
	Abstract = {Why do pro-democracy protests emerge in some countries at certain periods of time and not others? Pro-democracy protests, I argue, are more likely to arise when the economy is not performing well and people blame the autocratic nature of their regime for the economy, than when the economy is performing well, or when people do not blame the nature of their regime for the poor state of the economy. People are more likely to associate the economy with the nature of their regime, I further argue, in election periods, particularly when people are unable to remove the incumbent government from power through elections. My argument is supported by a statistical analysis of pro-democracy protests in 158 countries between 2006 and 2011, showing that not only is the economy an important factor explaining the emergence of pro-democracy protests, but that other factors commonly thought to affect these protests, including technologies like cell phones and the Internet, are not.},
	Author = {Brancati, Dawn},
	Date-Added = {2016-03-01 20:31:25 +0000},
	Date-Modified = {2016-03-01 20:31:42 +0000},
	Journal = {Comparative Political Studies},
	Number = {11},
