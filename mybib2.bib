@article{Houle2016,
	Abstract = {Does class inequality increase the risk of civil war? I posit that inequality between social classes affects civil wars through two pathways: (1) it heightens the risk of political violence by fueling distributive conflicts; and (2) it reduces structural coup-proofing, which, in turn, increases the capacity of the military to fight insurgents. Combining these effects implies that the net effect of class inequality on civil war is ambiguous. Although class inequality increases the propensity for violence, in unequal countries political violence rarely takes the form of wars because such countries have strong militaries. Class inequality, however, breeds other forms of political violence. In particular, it increases the likelihood of military coups. The two effects of class inequality reinforce each other in the case of coups: inequality simultaneously stirs distributional conflicts and increases the capacity of the military to mount coups by reducing coup-proofing. Using data on 128 developing countries between 1960 and 2008, I find that while class inequality fosters coups, it has no discernible effect on civil wars. I also provide evidence consistent with my causal mechanisms: (1) inequality creates greater threat to the rulers by fueling political instability; (2) inequality reduces structural coup-proofing; and (3) structural coup-proofing increases the likelihood of civil war.},
	Author = {Houle, Christian},
	Date-Added = {2016-08-03 15:43:07 +0000},
	Date-Modified = {2016-08-03 15:43:39 +0000},
	Doi = {10.1177/0022343316652187},
	Eprint = {http://jpr.sagepub.com/content/early/2016/07/26/0022343316652187.full.pdf+html},
	Journal = {Journal of Peace Research},
	Pages = {Forthcoming},
	Title = {Why class inequality breeds coups but not civil wars},
	Url = {http://jpr.sagepub.com/content/early/2016/07/26/0022343316652187.abstract},
	Year = {2016},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1177/0022343316652187}}

