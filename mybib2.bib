@article{Balcells2015,
	Annote = {10.1177/0010414015581681},
	Author = {Balcells, Laia and Fern{\'a}ndez-Albertos, Jos{\'e} and Kuo, Alexander},
	Date = {2015/05/05},
	Date-Added = {2015-06-19 17:25:33 +0000},
	Date-Modified = {2015-06-19 17:25:33 +0000},
	Journal = {Comparative Political Studies},
	Journal1 = {Comparative Political Studies},
	Month = {05},
	Title = {Preferences for Inter-Regional Redistribution},
	Ty = {JOUR},
	Url = {http://cps.sagepub.com/content/early/2015/05/04/0010414015581681.abstract N2 - What explains individual support for inter-regional redistribution? Few studies examine support for regional redistribution, even though such issues are politically salient in many democracies. We test models that hypothesize that such preferences are affected by regional and individual income, as well as other arguments beyond the traditional political economy framework. We focus on informational assumptions and implications of these models with an experiment embedded in a nationally representative survey in Spain. We randomly inform some citizens of the true relative income of their region to assess the impact of this information on regional redistribution preferences. We find that citizens' learning about their region's relative position affects these preferences in a manner consistent with some of the political economy models. We also find some support for out-group concerns as an important factor. The findings have implications for the applicability of economic models to explaining support for regional arrangements.},
	Year = {2015},
	Bdsk-Url-1 = {http://cps.sagepub.com/content/early/2015/05/04/0010414015581681.abstract%20N2%20-%20What%20explains%20individual%20support%20for%20inter-regional%20redistribution?%20Few%20studies%20examine%20support%20for%20regional%20redistribution,%20even%20though%20such%20issues%20are%20politically%20salient%20in%20many%20democracies.%20We%20test%20models%20that%20hypothesize%20that%20such%20preferences%20are%20affected%20by%20regional%20and%20individual%20income,%20as%20well%20as%20other%20arguments%20beyond%20the%20traditional%20political%20economy%20framework.%20We%20focus%20on%20informational%20assumptions%20and%20implications%20of%20these%20models%20with%20an%20experiment%20embedded%20in%20a%20nationally%20representative%20survey%20in%20Spain.%20We%20randomly%20inform%20some%20citizens%20of%20the%20true%20relative%20income%20of%20their%20region%20to%20assess%20the%20impact%20of%20this%20information%20on%20regional%20redistribution%20preferences.%20We%20find%20that%20citizens%E2%80%99%20learning%20about%20their%20region%E2%80%99s%20relative%20position%20affects%20these%20preferences%20in%20a%20manner%20consistent%20with%20some%20of%20the%20political%20economy%20models.%20We%20also%20find%20some%20support%20for%20out-group%20concerns%20as%20an%20important%20factor.%20The%20findings%20have%20implications%20for%20the%20applicability%20of%20economic%20models%20to%20explaining%20support%20for%20regional%20arrangements.}}

@article{Gonzalez-Ocantos2015,
	Annote = {10.1177/0010414015574882},
	Author = {Gonz{\'a}lez-Ocantos, Ezequiel and Kiewiet de Jonge, Chad and Nickerson, David W.},
	Date = {2015/04/01},
	Date-Added = {2015-06-19 17:22:50 +0000},
	Date-Modified = {2015-06-19 17:23:01 +0000},
	Journal = {Comparative Political Studies},
	Journal1 = {Comparative Political Studies},
	Month = {04},
	Title = {Legitimacy Buying: The Dynamics of Clientelism in the Face of Legitimacy Challenges},
	Ty = {JOUR},
	Url = {http://cps.sagepub.com/content/early/2015/03/26/0010414015574882.abstract N2 - Although elections have become the norm not only in democratic regimes but also in autocratic ones, the legitimacy of the electoral process in different countries is often contested. Facing strong international pressures to prove democratic credentials, eventual winners have a strong incentive to ensure high levels of voter turnout. Conversely, leaders of parties likely to lose the election have an incentive to reduce turnout---for example, through boycotts---to delegitimize the election. In such situations, turnout is a major dimension of competition. To overcome the potential delegitimizing effects of low turnout, incumbents will often turn toward clientelistic mobilization, as high turnout can be a powerful rejoinder to those who denounce elections as a sham and can put observers' concerns about the legitimacy of the electoral process to rest. We develop a theory to explain how such campaigns will target particularistic benefits. We argue that what we term ``legitimacy buying'' will be primarily aimed at ``staunching the bleeding'' of supporters who are usually consistent voters but have doubts about the legitimacy of the election. This theoretical prediction departs from theories used to explain vote buying in contexts in which the legitimacy of the elections is largely uncontested by significant groups in society. The theory is supported by data from a list experiment on vote buying conducted after the Honduran 2009 elections, which occurred in the wake of President Zelaya's ousting and in the midst of a boycott promoted by his supporters.},
	Year = {2015},
	Bdsk-Url-1 = {http://cps.sagepub.com/content/early/2015/03/26/0010414015574882.abstract%20N2%20-%20Although%20elections%20have%20become%20the%20norm%20not%20only%20in%20democratic%20regimes%20but%20also%20in%20autocratic%20ones,%20the%20legitimacy%20of%20the%20electoral%20process%20in%20different%20countries%20is%20often%20contested.%20Facing%20strong%20international%20pressures%20to%20prove%20democratic%20credentials,%20eventual%20winners%20have%20a%20strong%20incentive%20to%20ensure%20high%20levels%20of%20voter%20turnout.%20Conversely,%20leaders%20of%20parties%20likely%20to%20lose%20the%20election%20have%20an%20incentive%20to%20reduce%20turnout%E2%80%94for%20example,%20through%20boycotts%E2%80%94to%20delegitimize%20the%20election.%20In%20such%20situations,%20turnout%20is%20a%20major%20dimension%20of%20competition.%20To%20overcome%20the%20potential%20delegitimizing%20effects%20of%20low%20turnout,%20incumbents%20will%20often%20turn%20toward%20clientelistic%20mobilization,%20as%20high%20turnout%20can%20be%20a%20powerful%20rejoinder%20to%20those%20who%20denounce%20elections%20as%20a%20sham%20and%20can%20put%20observers%E2%80%99%20concerns%20about%20the%20legitimacy%20of%20the%20electoral%20process%20to%20rest.%20We%20develop%20a%20theory%20to%20explain%20how%20such%20campaigns%20will%20target%20particularistic%20benefits.%20We%20argue%20that%20what%20we%20term%20%E2%80%9Clegitimacy%20buying%E2%80%9D%20will%20be%20primarily%20aimed%20at%20%E2%80%9Cstaunching%20the%20bleeding%E2%80%9D%20of%20supporters%20who%20are%20usually%20consistent%20voters%20but%20have%20doubts%20about%20the%20legitimacy%20of%20the%20election.%20This%20theoretical%20prediction%20departs%20from%20theories%20used%20to%20explain%20vote%20buying%20in%20contexts%20in%20which%20the%20legitimacy%20of%20the%20elections%20is%20largely%20uncontested%20by%20significant%20groups%20in%20society.%20The%20theory%20is%20supported%20by%20data%20from%20a%20list%20experiment%20on%20vote%20buying%20conducted%20after%20the%20Honduran%202009%20elections,%20which%20occurred%20in%20the%20wake%20of%20President%20Zelaya%E2%80%99s%20ousting%20and%20in%20the%20midst%20of%20a%20boycott%20promoted%20by%20his%20supporters.}}

