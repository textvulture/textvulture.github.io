@article{Barbera2015,
	Annote = {10.1093/pan/mpu011},
	Author = {Barber{\'a}, Pablo},
	Date-Added = {2015-08-18 19:26:03 +0000},
	Date-Modified = {2015-08-19 17:57:34 +0000},
	Journal = {Political Analysis},
	Journal1 = {Political Analysis},
	Number = {1},
	Pages = {76--91},
	Title = {Birds of the Same Feather Tweet Together: Bayesian Ideal Point Estimation Using Twitter Data},
	Ty = {JOUR},
	Url = {http://pan.oxfordjournals.org/content/23/1/76.abstract N2 - Politicians and citizens increasingly engage in political conversations on social media outlets such as Twitter. In this article, I show that the structure of the social networks in which they are embedded can be a source of information about their ideological positions. Under the assumption that social networks are homophilic, I develop a Bayesian Spatial Following model that considers ideology as a latent variable, whose value can be inferred by examining which politics actors each user is following. This method allows us to estimate ideology for more actors than any existing alternative, at any point in time and across many polities. I apply this method to estimate ideal points for a large sample of both elite and mass public Twitter users in the United States and five European countries. The estimated positions of legislators and political parties replicate conventional measures of ideology. The method is also able to successfully classify individuals who state their political preferences publicly and a sample of users matched with their party registration records. To illustrate the potential contribution of these estimates, I examine the extent to which online behavior during the 2012 US presidential election campaign is clustered along ideological lines.},
	Volume = {23},
	Year = {2015},
	Bdsk-Url-1 = {http://pan.oxfordjournals.org/content/23/1/76.abstract%20N2%20-%20Politicians%20and%20citizens%20increasingly%20engage%20in%20political%20conversations%20on%20social%20media%20outlets%20such%20as%20Twitter.%20In%20this%20article,%20I%20show%20that%20the%20structure%20of%20the%20social%20networks%20in%20which%20they%20are%20embedded%20can%20be%20a%20source%20of%20information%20about%20their%20ideological%20positions.%20Under%20the%20assumption%20that%20social%20networks%20are%20homophilic,%20I%20develop%20a%20Bayesian%20Spatial%20Following%20model%20that%20considers%20ideology%20as%20a%20latent%20variable,%20whose%20value%20can%20be%20inferred%20by%20examining%20which%20politics%20actors%20each%20user%20is%20following.%20This%20method%20allows%20us%20to%20estimate%20ideology%20for%20more%20actors%20than%20any%20existing%20alternative,%20at%20any%20point%20in%20time%20and%20across%20many%20polities.%20I%20apply%20this%20method%20to%20estimate%20ideal%20points%20for%20a%20large%20sample%20of%20both%20elite%20and%20mass%20public%20Twitter%20users%20in%20the%20United%20States%20and%20five%20European%20countries.%20The%20estimated%20positions%20of%20legislators%20and%20political%20parties%20replicate%20conventional%20measures%20of%20ideology.%20The%20method%20is%20also%20able%20to%20successfully%20classify%20individuals%20who%20state%20their%20political%20preferences%20publicly%20and%20a%20sample%20of%20users%20matched%20with%20their%20party%20registration%20records.%20To%20illustrate%20the%20potential%20contribution%20of%20these%20estimates,%20I%20examine%20the%20extent%20to%20which%20online%20behavior%20during%20the%202012%20US%20presidential%20election%20campaign%20is%20clustered%20along%20ideological%20lines.}}

